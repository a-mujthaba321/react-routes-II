{
    "extends": "airbnb",
    "rules" :{
        "linebreak-style": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "import/first":0,
        "react/jsx-indent":0,
        "comma-dangle":0,
        "react/prop-types":0,
        "padded-blocks": 0,
        "jsx-a11y/anchor-is-valid":0,
        "class-methods-use-this":0,
        "jsx-a11y/label-has-for":0
    }
}